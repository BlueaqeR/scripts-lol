for i,v in pairs(game:GetService("Workspace").PlayerCharacters:GetChildren()) do
    myname = game.Players.LocalPlayer.Name
    if v.Name == myname then do
        v.ChildAdded:Connect(function(v)
            if v.Name == "FallDamage" then do
                v.Disabled = true
                v:Destroy()
            end
        end
    end)
end
end
end
