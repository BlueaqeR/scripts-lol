local ShowerLib = loadstring(game:HttpGet("https://fluxteam.xyz/scripts/others/ShowerHeadLibrary.lua"))()
local Window = ShowerLib:CreateWindow("F"); --Clicking this will make it rotate.
local HomePage = Window:Section("Main")
local Visuals = Window:Section("Visuals")
local Misc = Window:Section("Misc")
local char = game.Players.LocalPlayer.Character

function getRoot(char)
	local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
	return rootPart
end


HomePage:AddLabel("Minus Decendance")

HomePage:AddButton("Print Monsters (M)", function()
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(k)
		if k == "m" then
			for _,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
				if v:IsA("StringValue") and v.Name == "MonsterName" then do
						print(tostring(v.Value))
					end
				end
			end
		end
	end)

	game:GetService("Workspace").Enemies.ChildAdded:Connect(function(v)
		print("Monsters Have Spawned !")
	end)
end)

HomePage:AddButton("Press All Buttons (Risky) (L)", function()
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(k)
		if k == "l" then
			for i,v in pairs(workspace.Tasks:GetDescendants()) do
				if v:IsA("ClickDetector") and v.MaxActivationDistance == 20 then do
						wait()
						_G.spd = true
						if _G.spd == true then do
								local lastpos = getRoot(game.Players.LocalPlayer.Character).CFrame
								char.HumanoidRootPart.CFrame = v.Parent.CFrame + Vector3.new(0,40,0)
								wait(3)
								fireclickdetector(v)
								wait()
								char.HumanoidRootPart.CFrame = lastpos
								if char.Humanoid.Health == 0 then
									_G.spd = false
								end
							end
						end
					end
				end
			end
		end
	end)
end)

HomePage:AddButton("Collect All Notes (Broken?) (Z)", function()
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(k)
		if k == "z" then
			for i,v in pairs(workspace.Info:GetDescendants()) do
				if v:IsA("ClickDetector") then do
						wait()
						fireclickdetector(v)
						wait()
						print("Succesfully Collected All Info Pieces !")
					end
				end
			end
		end
	end)
end)

HomePage:AddButton("Close/Open All Doors (B)", function()
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(k)
		if k == "b" then
			for i,v in pairs(game:GetService("Workspace").ButtonPanels:GetDescendants()) do
				if v:IsA("ClickDetector") then do
						fireclickdetector(v)
					end
				end
			end
		end
	end)
end)

HomePage:AddButton("Break Game (Fly To The Map)", function()
	local prt = game.Players.LocalPlayer.Character.HumanoidRootPart:Clone()
	prt.Parent = game.Players.LocalPlayer.Character
	prt.Name = "bp"

	game.Players.LocalPlayer.Character.HumanoidRootPart:Destroy()
end)

HomePage:AddBind("Close Keybind", Enum.KeyCode.LeftControl, function()
	ShowerLib:OpenClose() -- opens or closes the UI
end)

HomePage:AddButton("Destroy GUI", function()
	ShowerLib:Destroy() -- Kills the GUI
end)

Visuals:AddButton("Monster ESP", function()
	function addUI(part)
		local partGui = Instance.new("BillboardGui", part)
		partGui.Size = UDim2.new(1,1,1)
		partGui.AlwaysOnTop = true
		partGui.Name = "Item-Esp"
		local frame = Instance.new("Frame", partGui)
		frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		frame.BackgroundTransparency = 0.3
		frame.Size = UDim2.new(1,1,1)
		frame.BorderSizePixel = 0
		local nameGui = Instance.new("BillboardGui", part)
		nameGui.Size = UDim2.new(3,0,1,5,0)
		nameGui.SizeOffset = Vector2.new(0,1)
		nameGui.AlwaysOnTop = true
		nameGui.Name = "Name"
		local text = Instance.new("TextLabel", nameGui)
		text.Text = "Monster"
		text.TextColor = Color3.fromRGB(255,80,60)
		text.TextTransparency(0.25)
		text.BackgroundTransparency = 0
		text.TextScaled = true
		text.Size = UDim2.new(1,0,1,0)
		text.Font = Enum.Font.GothamSemiBold
		text.Name = "Text"
	end

	for _,v in pairs(game:GetService("Workspace").Enemies.Monster:GetChildren()) do
		if v:FindFirstChild("HumanoidRootPart") then
			addUI(v.HumanoidRootPart)
		end
	end
	game:GetService("Workspace").Enemies.ChildAdded:Connect(function(v)
		addUI(v:WaitForChild("HumanoidRootPart"))
	end)
end)

Misc:AddButton("Infinite Yield", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Misc:AddButton("Enable Shiftlock", function()
	game.Players.LocalPlayer.DevEnableMouseLock = true
end)

Misc:AddButton("Full Bright", function()
	game:GetService("Lighting").Brightness = 2
	game:GetService("Lighting").ClockTime = 14
	game:GetService("Lighting").FogEnd = 100000
	game:GetService("Lighting").GlobalShadows = false
	game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
end)
